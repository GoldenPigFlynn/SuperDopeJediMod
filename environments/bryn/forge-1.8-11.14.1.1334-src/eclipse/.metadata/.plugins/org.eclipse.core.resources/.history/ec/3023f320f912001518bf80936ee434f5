package superDopeJediMod;

import java.util.Random;

import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.block.Block;
import net.minecraft.block.material.Material;
import net.minecraft.item.Item;

public class PowerCrystalOre extends BaseBlock {
	
	String color;
	
	protected PowerCrystalOre(String unlocalizedName)
	
	super(Material.rock, unlocalizedName, color);
	
public Item getItemDropped(int metadata, Random random, int fortune) {
	        
	    	return Item.getItemFromBlock(SuperDopeJediMod.brownSteel);
	    }
	
	

}
